    "format": "be-pretty formatAll && npm run tslint",
    "build": "rimraf dist && npm run format && npm run build-ts",
    "test": "npm run build && npm run test-unit && cross-env NODE_ENV=test npm run test-integration",
    "test-local": "npm run build && npm run test-unit && cross-env NODE_ENV=test_local npm run test-integration",
    "test-unit": "mocha --opts unit-mocha.opts",
    "test-integration": "npm run test-init && mocha --opts integration-mocha.opts --exit",
    "test-init": "node dist/test/initTest.js",
    "build-ts": "tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
